Posts! <br>
{{file-upload url="/upload"}}

{{#link-to "post-page"}}
	content
{{/link-to}}
<div class="timeline">
	{{#each model as |post|}}
		<div class="postCont">
			<p>Id: {{post.id}}</p>
			<h1>{{post.title}}</h1>
			
			{{post.image}} 
			<h3>{{post.text}}</h3> <br>
		</div>
	{{/each}}
</div>
{{outlet}}